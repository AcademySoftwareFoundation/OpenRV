{
  "name": "mu-language",
  "displayName": "Mu Language Support",
  "description": "Syntax highlighting and language support for Mu programming language",
  "version": "1.0.0",
  "publisher": "openrv-mu-language",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mu",
        "aliases": [
          "Mu",
          "mu"
        ],
        "extensions": [
          ".mu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mu",
        "scopeName": "source.mu",
        "path": "./syntaxes/mu.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AcademySoftwareFoundation/OpenRV"
  },
  "keywords": [
    "mu",
    "rv",
    "openrv",
    "autodesk",
    "vfx",
    "post-production"
  ],
  "license": "Apache-2.0"
}
